python manage.py runmodwsgi --setup-only --port=80 --user apache --group apache --server-root=/etc/mod_wsgi-express-80 --ssl-certificate-file /etc/letsencrypt/live/trademanager.org/cert.pem --ssl-certificate-key-file /etc/letsencrypt/live/trademanager.org/privkey.pem --ssl-certificate-chain-file /etc/letsencrypt/live/trademanager.org/fullchain.pem --https-port 443 --https-only --server-name trademanager.org --reload-on-changes
